#map{
  width: 100%;
  height: 100vh;
}
.input{
  position: fixed;
  width: 100%;
  bottom: 30rpx;
  left: 0;
  display: flex;
  justify-content: center;
}
.img{
  position: fixed;
  right: 30rpx;
  bottom: 200rpx;
}
.img image{
  width: 100rpx;
  height: 100rpx;
}
.detail{
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  z-index: 10;
}
.detail cover-image{
  width: 100%;
  height: 600rpx;
}
.detail .btn{
  display: flex;
}
.detail .btn button{
  width: 50%;
}
.detail .describe{
  background-color: white;
  font-size: 30rpx;
  color: black;
  text-align: center;
}